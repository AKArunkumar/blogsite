<% include partials/header %>
    <% blogs.forEach(function(blog){ %>
        <div class="ui raised segment">
            <p>
                <h2>
                    <%= blog.title %>
                </h2>
                <img src="<%= blog.image %>" alt="">
                <p>
                    <%= blog.created %>
                </p>
                <p>
                    <%= blog.body %>
                </p>
            </p>
        </div>
        <%})%>
 <% include partials/footer %>